<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.handong.android.soccerfield">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SoccerField">
        <activity
            android:name=".AdminOptions"
            android:exported="false" />
        <activity
            android:name=".LoginAdmin"
            android:exported="false" />
        <activity
            android:name=".CheckReservation"
            android:exported="false" />
        <activity
            android:name=".ReserveOK"
            android:exported="false" />
        <activity
            android:name=".ReserveConfirm"
            android:exported="false" />
        <activity
            android:name=".SoccerFieldInfo"
            android:exported="false" />
        <activity
            android:name=".ChooseField"
            android:exported="false" />
        <activity
            android:name=".FieldSearch"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>